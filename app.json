{
  "expo": {
    "name": "Beememo",
    "slug": "protonote-mobile-app",
    "version": "1.2.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "com.protonote.protonote",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.protonote.protonote",
      "buildNumber": "25"
    },
    "android": {
      "package": "com.protonote.protonote",
      "versionCode": 22,
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b539128c-58b8-4141-9f87-ce7730b08730"
      }
    },
    "plugins": [
      "expo-apple-authentication",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33
          }
        }
      ]
    ]
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-4452701921985328~5222777567",
    "ios_app_id": "ca-app-pub-4452701921985328~5920127625"
  }
}
